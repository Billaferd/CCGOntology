}, {
  "@id" : "https://example.org/ccgCard",
  "@type" : [ "http://www.w3.org/2002/07/owl#NamedIndividual" ],
  "http://purl.org/dc/terms/description" : [ {
    "@language" : "en",
    "@value" : "The most basic part of a CCG is the Card. These can take many shapes and sizes, and they are the primary means by which players interact with the game."
  } ],
  "http://www.w3.org/1999/02/22-rdf-syntax-ns#type" : [ {
    "@id" : "http://www.w3.org/2002/07/owlClass"
  }, {
    "@id" : "http://www.w3.org/2004/02/skos/coreConcept"
  } ],
  "http://www.w3.org/2000/01/rdf-schemacomment" : [ {
    "@language" : "en",
    "@value" : "Cards are the building blocks of any CCG. They represent characters, actions, events, resources, and other elements that are integral to the game's mechanics. Each card typically has attributes such as name, type, cost, and effects that define its role within the game. Cards are collected, traded, and used in gameplay to achieve objectives and compete against opponents."
  } ],
  "http://www.w3.org/2004/02/skos/coredefinition" : [ {
    "@language" : "en",
    "@value" : "In the context of a Collectible Card Game, a card is a physical or digital artifact that represents a game element with various attributes and abilities. It is used by players to perform actions and strategies during play."
  } ],
  "http://www.w3.org/2004/02/skos/coreeditorialNote" : [ {
    "@language" : "en",
    "@value" : "Ensure that the card class is flexible enough to accommodate the diverse range of cards found across various CCGs, from simple to complex."
  } ],
  "http://www.w3.org/2004/02/skos/coreexample" : [ {
    "@language" : "en",
    "@value" : "A 'Fireball' spell card that costs 3 mana to cast and deals 4 damage to any target."
  } ],
  "http://www.w3.org/2004/02/skos/corehistoryNote" : [ {
    "@language" : "en",
    "@value" : "The concept of the card has evolved over time, with early CCGs featuring simple designs and mechanics, while modern CCGs offer a wide range of complex and thematic cards."
  } ],
  "http://www.w3.org/2004/02/skos/coreprefLabel" : [ {
    "@language" : "en",
    "@value" : "Card"
  } ],
  "http://www.w3.org/2004/02/skos/corescopeNote" : [ {
    "@language" : "en",
    "@value" : "This class can be extended to include various subclasses representing different categories of cards found in specific CCGs, such as creature cards, spell cards, or equipment cards."
  } ]
